{
    "manifest_version": 2,
    "name": "My Extension",
    "description": "build an extension",
    "version": "1.0.0",
    "content_scripts": [{
        "matches": [
            "<all_urls>"
        ],

        "js": ["content.js"]

    }],
    "chrome_url_overrides": {
        "newtab": "ce.html"
    }



}